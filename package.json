{
  "type": "module",
  "name": "fuzzy-dist",
  "version": "0.0.1",
  "description": "Over simplistic fuzzy search library using Damerau-Levenshtein and optionally the keyboard layout to calculate distances.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint src test",
    "format": "prettier \"{,**/}*.{ts,md}\" --check",
    "format:fix": "prettier \"{,**/}*.{ts,md}\" --write",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "prepare": "husky",
    "publishToNPM": "nmp test && npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remotal-io/fuzzy-dist.git"
  },
  "keywords": [
    "fuzzy",
    "search",
    "Damerau",
    "Levenshtein",
    "distance",
    "algorithm",
    "string",
    "leven",
    "match"
  ],
  "author": "MickaÃ«l Cassy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/remotal-io/fuzzy-dist/issues"
  },
  "homepage": "https://github.com/remotal-io/fuzzy-dist#readme",
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0",
    "vitest": "^2.0.5"
  },
  "lint-staged": {
    "*.{ts,md}": "prettier --write --ignore-unknown"
  }
}
